a() {\n    return {\n      interests: ['Hip-Hop', 'Rock', 'Jazz', 'Techno'],\n      songs: [\n          {\n            title: 'Its Now or Never',\n            artist: 'Elvis Presley',\n            sales: '21000000'\n          },{\n            title: 'Uptown Funk',\n            artist: 'Mark Ronson',\n            sales: '20000000'\n          },\n      ]\n    }\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\nli {\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  color: #2c3e50;\n  margin-top: 10px;\n}\n\nh1 {\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]}]}